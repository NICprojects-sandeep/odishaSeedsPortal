    <div class="container-fluid">
        <div class="row">

            <div class="col-md-12 p-sm-5">
                <form name="form" role="form">
                    <div class="form-row">
                        <div class="form-group col-md-2">
                        </div>
                        <div class="form-group col-md-2">
                            <label for="EarlierPI">
                                <b>Year<span style="color:red">*</span>:</b><br />
                            </label>
                            <select class="form-control" name="year" ng-model="year" ng-change="loadSeason();" required>
                                <option value="">--Select--</option>
                                <option value="2022-23">2022-23</option>

                            </select>
                        </div>
                        <div class="form-group col-md-2">
                            <label for="EarlierPI">
                                <b>Season<span style="color:red">*</span>:</b><br />
                            </label>
                            <select class="form-control" name="season" ng-model="season" ng-change="loadAllCrop()"
                                required>
                                <option value="">--Select-- </option>
                                <option ng-repeat="i in seasons" value="{{i.season}}">{{i.season}}</option>
                            </select>
                        </div>
                        <div class="form-group col-md-2">
                            <label for="EarlierPI">
                                <b>Crop<span style="color:red">*</span>:</b><br />
                            </label>
                            <select class="form-control" ng-model="crop">
                                <option value="" disabled>--Select-- </option>
                                <option ng-repeat="i in crops" value="{{i}}">{{i}}</option>
                            </select>
                        </div>

                        <div class="form-group col-md-2">
                            <label>Block:</label>
                            <select class="form-control" ng-model="block">
                                <option value="" disabled>--Select-- </option>
                                <option value="all">All </option>
                                <option ng-repeat="i in blocks" value="{{i.blockCode}}">{{i.blockName}}</option>
                            </select>
                        </div>
                        <div class="form-group col-md-2">
                        </div>
                    </div>
                </form>
            </div>
